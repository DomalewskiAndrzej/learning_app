{
  "projectType": "library",
  "sourceRoot": "libs/app/shared/data-access/src",
  "prefix": "app",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/app/to-do/ui-to-do-table"],
      "options": {
        "jestConfig": "libs/app/to-do/ui-to-do-table/jest.config.ts",
        "outputs": ["coverage/libs/app/shared/data-access"],
