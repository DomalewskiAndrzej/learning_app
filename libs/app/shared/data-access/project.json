      "lint": {
        "executor": "@nrwl/linter:eslint",
        "options": {
          "lintFilePatterns": [
            "libs/app/to-do/ui-to-do-table/**/*.ts",
            "libs/app/to-do/ui-to-do-table/**/*.html",
            "libs/app/shared/data-access/**/*.ts",
            "libs/app/shared/data-access/**/*.html"
          ]
        }
      }
    },
    "tags": []
  }
